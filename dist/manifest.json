{
    "manifest_version": 3, 
    "name": "Chrome Extension ecoprompt",
    "description": "Extension using React and webpack",
    "version":"1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
       "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["contentScript.js"]
        }
    ]

}